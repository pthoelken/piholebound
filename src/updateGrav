grep -iE '^https?:\/\/[a-z0-9.]{2,30}\.[a-z]{2,6}\b([-a-z0-9_\-\+.~#?&/=]*/)' /etc/pihole/adlists.list
if [ $? = 0 ]; then
       
        while read p; do
            sqlite3 /etc/pihole/gravity.db "INSERT INTO "adlist" ("address","enabled","comment") VALUES ('$p','1','Automatic Insert');"
        done </etc/pihole/adlists.list
fi